{
    "manifestVersion": 1,
    "id": "infrasync-board-extension-dev",
    "publisher": "InfrasyncInc",
    "version": "1.0.13",
    "name": "Create Dependency Epic",
    "description": "Azure DevOps extension to create a dependency epic from a work item.",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}
